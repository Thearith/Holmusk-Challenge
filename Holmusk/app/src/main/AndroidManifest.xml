<?xml version="1.0" encoding="utf-8"?>
<manifest package="holmusk.com.holmuskchallenge"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MainTheme">
        <activity android:name=".Activities.MainActivity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Activities.DailyActivity.DailyActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="holmusk.com.holmuskchallenge.Activities.MainActivity.MainActivity" />
        </activity>
    </application>

</manifest>
